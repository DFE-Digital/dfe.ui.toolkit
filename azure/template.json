{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "cdnProfileName": {
            "type": "string",
            "metadata": {
                "description": "Name of Content Delivery Network profile"
            }
        },
        "cdnEndPointName": {
            "type": "string"
        },
        "customDomainName": {
            "defaultValue": "",
            "type": "string"
        },
        "originHostName": {
            "type": "string"
        },
        "originPath": {
            "type": "string",
            "defaultValue": ""
        }
    },
    "variables": {},
    "resources": [
        {
            "apiVersion": "2017-05-10",
            "name": "cdn-endpoint",
            "type": "Microsoft.Resources/deployments",
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "uri": "https://raw.githubusercontent.com/DFE-Digital/login.dfe.infrastructure/master/Shared/azure-cdn-endpoint.json",
                    "contentVersion": "1.0.0.0"
                },
                "parameters": {
                    "cdnEndPointName": {
                        "value": "[parameters('cdnEndPointName')]"
                    },
                    "cdnProfileName": {
                        "value": "[parameters('cdnProfileName')]"
                    },
                    "originHostName": {
                        "value": "[parameters('originHostName')]"
                    },
                    "customDomainName": {
                        "value": "[parameters('customDomainName')]"
                    },
                    "originPath": {
                        "value": "[parameters('originPath')]"
                    }
                }
            }
        }
    ]
}